
{
  "name": "InspireMe Chrome extension",
  "version": "1.0",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "manifest_version": 2,
  "content_scripts": [{
      "matches": ["<all_urls>"],
      "css": ["css/style.css"],
      "js": ["js/main.js", "js/jquery-3.3.1.min.js"]
    }]
}